<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.fansfunding.fan.project.activity.ProjectDetailWebActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar_project_detail_web"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        app:theme="@style/toolbar_white_theme"
        >

    </android.support.v7.widget.Toolbar>
    <com.fansfunding.verticalslide.CustWebView
        android:id="@+id/web_project_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbar_project_detail_web">
    </com.fansfunding.verticalslide.CustWebView>

</RelativeLayout>
